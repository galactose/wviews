 eligible(howard_moon) :- highGPA(howard_moon).#an interview program
eligible(howard_moon) :- minority(howard_moon), fairGPA(howard_moon). 
~eligible(howard_moon) :- ~fairGPA(howard_moon), ~highGPA(howard_moon).
 interview(howard_moon) :- ~Keligible(howard_moon), ~K~eligible(howard_moon).
fairGPA(howard_moon) v highGPA(howard_moon).